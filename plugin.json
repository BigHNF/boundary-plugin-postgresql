{
    "description" : "Collects statistics from PostgreSQL",
    "command" : "python postgres_plugin.py $(host) $(port) $(database) $(username) $(password) $(source)",
    "icon" : "icon.png",
    "metrics" : ["LF_PSQL_EXLocks"],
    "metrics" : ["LF_PSQL_RowExclusive"],
    "metrics" : ["LF_PSQL_ShareRowExclusive"],
    "metrics" : ["LF_PSQL_ShareUpdateExclusive"],
    "metrics" : ["LF_PSQL_Share"],
    "metrics" : ["LF_PSQL_AccessShare"],
    "metrics" : ["LF_PSQL_ChkPnt_Wrt_Time"],
	"metrics" : ["LF_PSQL_ChkPnts_Timed"],
	"metrics" : ["LF_PSQL_Buffers_Alloc"],
	"metrics" : ["LF_PSQL_Buffers_Clean"],
	"metrics" : ["LF_PSQL_Buffers_Backnd_fSync"],
	"metrics" : ["LF_PSQL_ChkPnt_Sync_Time"],
	"metrics" : ["LF_PSQL_ChkPnts_Req"],
	"metrics" : ["LF_PSQL_Buffers_Backend"],
	"metrics" : ["LF_PSQL_MaxWritten_Clean"],
	"metrics" : ["LF_PSQL_Buffers_ChkPnt"],
	"metrics" : ["LF_PSQL_blks_read"],
	"metrics" : ["LF_PSQL_disk_size"],
	"metrics" : ["LF_PSQL_xact_commit"],
	"metrics" : ["LF_PSQL_tup_deleted"],
	"metrics" : ["LF_PSQL_xact_rollback"],
	"metrics" : ["LF_PSQL_blks_hit"],
	"metrics" : ["LF_PSQL_tup_returned"],
	"metrics" : ["LF_PSQL_tup_fetched"],
	"metrics" : ["LF_PSQL_tup_updated"],
	"metrics" : ["LF_PSQL_tup_inserted"],

    "paramSchema" : [
        {
            "title" : "host",
            "description" : "database host name or IP",
            "type" : "string"
            
        },
        {
            "title" : "port",
            "description" : "Postgres Port",
            "type" : "integer"
            
        },
        {
            "title" : "database",
            "description" : "database name",
            "type" : "string"
            
        },
        {
            "title" : "username",
            "description" : "Postgres username",
            "type" : "string"
            
        },
        {
            "title" : "password",
            "description" : "Postgres Port",
            "type" : "password"
            
        },
        {
            "title" : "source",
            "description" : "display name",
            "type" : "string"
            
        }
    ]
}