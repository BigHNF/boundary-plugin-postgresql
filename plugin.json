{
    "description" : "Collects statistics from PostgreSQL Database",
    "command" : "python postgres_plugin.py $(host) $(port) $(database) $(username) $(password) $(source)",
    "icon" : "icon.png",
    "metrics" : [
    	"PSQL_EXLocks",
    	"PSQL_RowExclusive",
 	   	"PSQL_ShareRowExclusive",
  	  	"PSQL_ShareUpdateExclusive",
  	  	"PSQL_Share",
  	  	"PSQL_AccessShare",
  	  	"PSQL_ChkPnt_Wrt_Time",
		"PSQL_ChkPnts_Timed",
		"PSQL_Buffers_Alloc",
		"PSQL_Buffers_Clean",
		"PSQL_Buffers_Backnd_fSync",
		"PSQL_ChkPnt_Sync_Time",
		"PSQL_ChkPnts_Req",
		"PSQL_Buffers_Backend",
		"PSQL_MaxWritten_Clean",
		"PSQL_Buffers_ChkPnt",
		"PSQL_blks_read",
		"PSQL_disk_size",
		"PSQL_xact_commit",
		"PSQL_tup_deleted",
		"PSQL_xact_rollback",
		"PSQL_blks_hit",
		"PSQL_tup_returned",
		"PSQL_tup_fetched",
		"PSQL_tup_updated",
		"PSQL_tup_inserted"
	],

    "paramSchema" : [
        {
            "title" : "host",
            "description" : "database host name or IP",
            "type" : "string"
            
        },
        {
            "title" : "port",
            "description" : "Postgres Port",
            "type" : "integer"
            
        },
        {
            "title" : "database",
            "description" : "database name",
            "type" : "string"
            
        },
        {
            "title" : "username",
            "description" : "Postgres username",
            "type" : "string"
            
        },
        {
            "title" : "password",
            "description" : "Postgres Port",
            "type" : "password"
            
        },
        {
            "title" : "source",
            "description" : "display name",
            "type" : "string"
            
        }
    ]
}